
     <div id="panel-158939" class="panel-group">
       <div tal:repeat="p processes" class="panel panel-default">
	 <div class="panel-heading">
	   <a  href="#panel-element-${p['process'].__name__.capitalize()}" data-parent="#panel-158939" data-toggle="collapse" class="panel-title">
              <span class="glyphicon glyphicon-log-in"> </span>  
	   </a>
           <a tal:attributes="href p['url']" class="nav navbar-nav navbar-right">${p['process'].title.capitalize()}</a>
	 </div>
	 <div tal:define="p_id p['process'].title.capitalize()" class="panel-collapse collapse" id="panel-element-${p['process'].__name__.capitalize()}" style="height: auto;">
           <div  tal:define="startedps p['process'].started_processes" class="panel-body">
                                <dl>
			          <dt>Description</dt>
			           <dd><p class="text-default">${p['process'].description}</p></dd>
                                 </dl>

           <ul tal:define="start p['nav_bar']['start']; links start['links']; groups start['groups']" class="nav nav-list">
              <li tal:condition="links" tal:repeat="l links" >
                 <a href="${l.url}" data-toggle="tooltip" data-placement="bottom" title="${l.action.informations}"><span class="glyphicon glyphicon-play-circle"></span>${l.title}</a>
              </li>
              <li tal:condition="groups" tal:repeat="(k, g) groups.iteritems()">
              <div class="btn-group">
               <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                 <span class="glyphicon glyphicon-play-circle"></span> ${k} <span class="caret"></span>
               </button>
              <ul class="dropdown-menu" role="menu" style="position: relative">
              <li tal:repeat="a g">
                 <a href="${a.url}" data-toggle="tooltip" data-placement="bottom" title="${a.action.informations}">${a.title}</a>
              </li>
              </ul>
              </div>
              </li>
           </ul>

           <ul tal:define="active p['nav_bar']['active']; links active['links']; groups active['groups']" class="nav nav-list">
              <li tal:condition="links" tal:repeat="l links" >
                 <a href="${l.url}" data-toggle="tooltip" data-placement="bottom" title="${l.action.informations}">${l.title}</a>
              </li>
              <li tal:condition="groups" tal:repeat="(k, g) groups.iteritems()">
              <div class="btn-group">
               <button type="button" class="btn btn-primary dropdown-toggle" data-toggle="dropdown">
                 ${k} <span class="caret"></span>
               </button>
              <ul class="dropdown-menu" role="menu" style="position: relative">
              <li tal:repeat="a g">
                 <a href="${a.url}" data-toggle="tooltip" data-placement="bottom" title="${a.action.informations}">${a.title}</a>
              </li>
              </ul>
              </div>
              </li>
           </ul>

          </div>
        </div>
      </div>
    </div>
